{
    "taskGroups": [
      {
        "taskSpec": {
          "runnables": [
            {
              "container": {
                "imageUri": "gcr.io/uas-orchestration-engine/orchestration-images/uas-worker:latest",
                "entrypoint": "/bin/bash",
                "commands": [
                  "-c",
                  "conda run -n harvest nvidia-smi"
                ]
              }
            }
          ],
          "computeResource": {
            "cpuMilli": 4000,
            "memoryMib": 15360
          }
        },
        "taskCount": 1,
        "parallelism": 1
      }
    ],
    "allocationPolicy": {
      "instances": [
        {
          "policy": {
            "machineType": "n1-standard-4",
            "accelerators": [
              {
                "type": "nvidia-tesla-t4",
                "count": 1
              }
            ],
            "provisioningModel": "STANDARD"
          }
        }
      ]
    },
    "logsPolicy": {
      "destination": "CLOUD_LOGGING"
    }
  }